<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="5dp"
    android:layout_marginRight="5dp"
    android:layout_marginTop="5dp"
    android:layout_marginBottom="25dp"
    android:paddingVertical="@dimen/my_page_write_item_padding_vertical"
    android:clickable="true"
    android:background="@drawable/common_corner_round_white_01">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/thumbnail_layout"
        android:layout_width="match_parent"
        android:layout_height="105dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent">
        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/img_room"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/common_background_f8f8fa"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/seminar_situation_layout"
            android:layout_width="50dp"
            android:layout_height="24dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/seminar_status_now"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/seminar_situation"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:textColor="@color/white"
                android:gravity="center"
                android:textSize="10sp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintRight_toLeftOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                tools:text="진행중"
                />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/seminar_my_create_layout"
            android:layout_width="40dp"
            android:layout_height="24dp"
            android:layout_marginLeft="8dp"
            android:background="@drawable/seminar_my_create"
            app:layout_constraintTop_toTopOf="@id/seminar_situation_layout"
            app:layout_constraintLeft_toRightOf="@id/seminar_situation_layout"
            app:layout_constraintBottom_toBottomOf="@id/seminar_situation_layout">
            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/seminar_my"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:textColor="@color/tory_white"
                android:gravity="center"
                android:textSize="10sp"
                android:paddingBottom="2dp"
                android:text="@string/my"
                android:fontFamily="@font/noto_sans_regular"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintRight_toLeftOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                />
        </androidx.constraintlayout.widget.ConstraintLayout>


        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/icon_lock"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/icon_lock"
            android:layout_marginRight="13dp"
            android:layout_marginTop="8dp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/image_layout"
            android:layout_width="44dp"
            android:layout_height="44dp"
            android:layout_marginLeft="11dp"
            android:layout_marginBottom="8dp"
            android:background="@drawable/common_corner_round_white_01"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintBottom_toBottomOf="parent">
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/img_teacher"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/seminar_max_count"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="100"
            android:textSize="12sp"
            android:layout_marginRight="10dp"
            android:layout_marginBottom="8dp"
            android:fontFamily="@font/noto_sans_regular"
            android:textColor="@color/tory_white"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            tools:text="100"
            />
        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/divide"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="12sp"
            android:text="/"
            android:layout_marginRight="3dp"
            android:fontFamily="@font/noto_sans_regular"
            android:textColor="@color/tory_white"
            app:layout_constraintRight_toLeftOf="@id/seminar_max_count"
            app:layout_constraintTop_toTopOf="@id/seminar_max_count"
            app:layout_constraintBottom_toBottomOf="@id/seminar_max_count"
            tools:text="/"
            />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/seminar_count"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="12sp"
            android:text="100"
            android:layout_marginRight="3dp"
            android:fontFamily="@font/noto_sans_semi_bold"
            android:textColor="@color/tory_white"
            app:layout_constraintRight_toLeftOf="@id/divide"
            app:layout_constraintTop_toTopOf="@id/divide"
            app:layout_constraintBottom_toBottomOf="@id/divide"
            tools:text="100"
            />

        <androidx.appcompat.widget.AppCompatImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/icon_white_people"
            android:layout_marginRight="6dp"
            app:layout_constraintRight_toLeftOf="@id/seminar_count"
            app:layout_constraintTop_toTopOf="@id/seminar_count"
            app:layout_constraintBottom_toBottomOf="@id/seminar_count"
            />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <!--
    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/join"
        android:layout_width="68dp"
        android:layout_height="32dp"
        android:textSize="@dimen/common_text_font_14"
        android:textColor="@color/tory_white"
        android:background="@drawable/common_corner_round_purple_02"
        android:gravity="center"
        android:paddingBottom="2dp"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/noto_sans_regular"
        style="?android:attr/borderlessButtonStyle"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/thumbnail_layout"/>
    -->

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/seminar_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="@dimen/common_text_font_14"
        android:lines="1"
        android:ellipsize="end"
        android:textColor="@color/tory_black"
        android:fontFamily="@font/noto_sans_semi_bold"
        android:layout_marginRight="5dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/thumbnail_layout"
        tools:text="취업정보센터"
        />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/detail_comment"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="2dp"
        android:textSize="@dimen/common_text_font_10"
        android:lines="1"
        android:ellipsize="end"
        android:textColor="@color/color_9e9e9e"
        android:fontFamily="@font/noto_sans_regular"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/seminar_title"
        tools:text="취업 걱정은 노!!afafdafafafdafafadaadsafdafafaf"
        />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/seminar_date"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="@dimen/common_text_font_10"
        android:lines="1"
        android:textColor="@color/color_9e9e9e"
        android:fontFamily="@font/noto_sans_regular"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/detail_comment"
        app:layout_constraintRight_toRightOf="parent"
        tools:text="05월 20일 (화) 14:00 ~ 05월 20일 (화) 17:00"
        />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/join"
        android:layout_width="match_parent"
        android:layout_height="32dp"
        android:textSize="@dimen/common_text_font_14"
        android:textColor="@color/tory_white"
        android:background="@drawable/common_corner_round_purple_02"
        android:gravity="center"
        android:paddingBottom="2dp"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/noto_sans_regular"
        style="?android:attr/borderlessButtonStyle"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/seminar_date"
        app:layout_constraintBottom_toBottomOf="parent"
        />

</androidx.constraintlayout.widget.ConstraintLayout>