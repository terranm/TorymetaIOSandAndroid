                            <androidx.appcompat.widget.AppCompatImageView
                                android:layout_width="match_parent"
                                android:layout_height="0dp"
                                android:background="@drawable/ttc_banner"
                                app:layout_constraintTop_toTopOf="parent"
                                app:layout_constraintLeft_toLeftOf="parent"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintRight_toRightOf="parent"
                                />

                        </androidx.constraintlayout.widget.ConstraintLayout>

                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/main_event_layout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="36dp"
                        android:paddingHorizontal="@dimen/common_padding_horizontal"
                        app:layout_constraintHorizontal_bias="0.0"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintRight_toRightOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/online_lecture">

                        <androidx.appcompat.widget.AppCompatTextView
                            android:id="@+id/event_title"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/noto_sans_semi_bold"
                            android:text="@string/event"
                            android:textColor="@color/tory_black"
                            android:textSize="@dimen/common_text_font_16"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <androidx.viewpager2.widget.ViewPager2
                            android:id="@+id/viewPager"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/main_event_height"
                            android:layout_marginTop="12dp"
                            android:orientation="horizontal"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintRight_toRightOf="parent"
                            app:layout_constraintTop_toBottomOf="@id/event_title" />

                        <me.relex.circleindicator.CircleIndicator3
                            android:id="@+id/indicator"
                            android:layout_width="match_parent"
                            android:layout_height="20dp"
                            android:layout_marginBottom="2dp"
                            app:ci_drawable="@drawable/main_indicator_selected"
                            app:ci_drawable_unselected="@drawable/main_indicator_unselected"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintRight_toRightOf="parent" />

                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/main_2d_campus_layout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="34dp"
                        android:layout_marginBottom="28dp"
                        android:paddingHorizontal="@dimen/common_padding_horizontal"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintRight_toRightOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/main_event_layout">
                        <androidx.appcompat.widget.AppCompatTextView
                            android:id="@+id/campus_title"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/campus_net_title"
                            android:textSize="@dimen/common_text_font_16"
                            android:textColor="@color/tory_black"
                            android:fontFamily="@font/noto_sans_semi_bold"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintTop_toTopOf="parent"/>

                        <androidx.constraintlayout.widget.Guideline
                            android:id="@+id/guideline"
                            android:layout_width="wrap_content"
                            android:layout_height="1dp"
                            android:orientation="vertical"
                            app:layout_constraintGuide_percent="0.5" />

                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:layout_width="0dp"
                            android:layout_height="@dimen/main_event_height"
                            android:layout_marginRight="@dimen/main_campus_button_guide_margin"
                            android:layout_marginTop="12dp"
                            android:layout_marginBottom="24dp"
                            android:background="@drawable/main_campus_map"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintRight_toLeftOf="@id/guideline"
                            app:layout_constraintTop_toBottomOf="@id/campus_title"
                            app:layout_constraintBottom_toBottomOf="parent">

                            <androidx.appcompat.widget.AppCompatTextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textSize="@dimen/main_campus_text_font_size"
                                android:gravity="center"
                                android:textColor="@color/tory_black"
                                android:layout_marginTop="@dimen/main_campus_text_margin_top"
                                android:fontFamily="@font/noto_sans_semi_bold"
                                android:text="@string/campus_detail_comment"
                                app:layout_constraintTop_toTopOf="parent"
                                app:layout_constraintLeft_toLeftOf="parent"
                                app:layout_constraintRight_toRightOf="parent" />

                            <androidx.appcompat.widget.AppCompatButton
                                android:id="@+id/campus_layout"
                                android:layout_width="64dp"
                                android:layout_height="24dp"
                                android:text="@string/visit_comment"
                                android:textSize="@dimen/common_text_font_12"
                                android:textColor="@color/tory_white"
                                android:gravity="center_vertical|center_horizontal"
                                android:background="@drawable/common_corner_round_indigo_01"
                                style="?android:attr/borderlessButtonStyle"
                                android:paddingBottom="2dp"
                                android:layout_marginBottom="@dimen/main_campus_button_margin_bottom"
                                app:layout_constraintLeft_toLeftOf="parent"
                                app:layout_constraintRight_toRightOf="parent"
                                app:layout_constraintBottom_toBottomOf="parent"/>

                        </androidx.constraintlayout.widget.ConstraintLayout>

                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:layout_width="0dp"
                            android:layout_height="@dimen/main_event_height"
                            android:layout_marginLeft="@dimen/main_campus_button_guide_margin"
                            android:layout_marginTop="12dp"
                            android:layout_marginBottom="24dp"
                            android:background="@drawable/main_nft_bg"
                            app:layout_constraintRight_toRightOf="parent"
                            app:layout_constraintLeft_toRightOf="@id/guideline"
                            app:layout_constraintTop_toBottomOf="@id/campus_title"
                            app:layout_constraintBottom_toBottomOf="parent">

                            <androidx.appcompat.widget.AppCompatTextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textSize="@dimen/main_campus_text_font_size"
                                android:gravity="center"
                                android:textColor="@color/tory_white"
                                android:layout_marginTop="@dimen/main_campus_text_margin_top"
                                android:text="@string/nft_detail_comment"
                                android:fontFamily="@font/noto_sans_semi_bold"
                                app:layout_constraintTop_toTopOf="parent"
                                app:layout_constraintLeft_toLeftOf="parent"
                                app:layout_constraintRight_toRightOf="parent" />

                            <androidx.appcompat.widget.AppCompatButton
                                android:id="@+id/nft_layout"
                                android:layout_width="64dp"
                                android:layout_height="24dp"
                                android:text="@string/enter_comment"
                                android:textSize="@dimen/common_text_font_12"
                                android:gravity="center"
                                android:textColor="@color/tory_black"
                                android:layout_marginBottom="@dimen/main_campus_button_margin_bottom"
                                android:paddingBottom="2dp"
                                android:background="@drawable/nft_enter_bg"
                                app:layout_constraintLeft_toLeftOf="parent"
                                app:layout_constraintRight_toRightOf="parent"
                                app:layout_constraintBottom_toBottomOf="parent"
                                />

                        </androidx.constraintlayout.widget.ConstraintLayout>

                    </androidx.constraintlayout.widget.ConstraintLayout>

                </androidx.constraintlayout.widget.ConstraintLayout>

            </androidx.core.widget.NestedScrollView>

            <androidx.coordinatorlayout.widget.CoordinatorLayout
                android:id="@+id/layout_coordinator"
                android:layout_width="match_parent"
                android:layout_height="76dp"
                android:background="@color/tory_white"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent">

                <com.tnmeta.torymeta.control.ToryLoungeButton
                    android:id="@+id/tory_lounge"
                    android:layout_width="match_parent"
                    android:layout_height="56dp"
                    android:layout_marginLeft="16dp"
                    android:layout_marginRight="16dp"
                    android:layout_marginTop="4dp"
                    android:clickable="true"
                    app:back_ground="@drawable/tory_lounge_button"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent" />

            </androidx.coordinatorlayout.widget.CoordinatorLayout>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.swiperefreshlayout.widget.SwipeRefreshLayout>

    <com.pnikosis.materialishprogress.ProgressWheel
        android:id="@+id/loadingProgress"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginTop="20dp"
        android:visibility="visible"
        app:matProg_barColor="@color/tory_purple"
        app:matProg_progressIndeterminate="true"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>